
<div class="container">
  <h1> <strong>Your articles</strong> </h1>
  <% @articles.each do |article| %>
  <div class="card-article">
    <h2> <%= link_to article.title, article_path(article) %> </h2>
    <p><%= article.content %> <%= link_to ' &#9998;'.html_safe, edit_article_path(article) %>
      <%= link_to '&#128465;'.html_safe, article_path(article), method: :delete  %> </p>
  </div>
    <% end %>
</div>
